gunit Esql;

ID:
    "X" OK
    "XYZ" OK
    "Xyz" OK
    "XyZ" OK
    "_X" OK
    "1X" FAIL
INT:
    "1" OK

atom    :
   "10" -> "10"
   "\"Hello\"" -> "\"Hello\""
   "Var" -> "Var"
   "TRUE" -> "TRUE"
   "FALSE" -> "FALSE"

sc_expr:
    "1 > 2" -> (> 1 2)
    "1 + 3 > 10" -> (> (+ 1 3) 10)

concat_expr:
    "\"Hello\" || \", world\"" -> (|| "\"Hello\"" "\", world\"")

add_expr:
   "1 + 3" -> (+ 1 3)
   "3 - 1" -> (- 3 1)
   "1 + 2 + 3 - 4" -> (- (+ (+ 1 2) 3) 4)
   " a + x" -> (+ a x)
   "x + 10" -> ( + x 10)
   "(1 + 2) + (5 + 6)" -> ( + (+ 1 2) (+ 5 6))
   "(1 + x) + (x - 6)" -> ( + (+ 1 x) (- x 6))
   "1 - 3/4 + 5" -> (+ (- 1 (/ 3 4)) 5)

mult_expr:
    "5 * 6" -> (* 5 6)
    "6 / 10" -> (/ 6 10)
    "1 * 2 * 3 * 4" -> (* (* (* 1 2) 3) 4)



